{
  "expo": {
    "name": "InaMova",
    "slug": "InaMova",
    "version": "1.0.0",
    "scheme": "inamova",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#134E4A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tbits.InaMova"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#134E4A"
      },
      "package": "com.tbits.InaMova"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
